    "react-hook-form": "7.65.0",
    "sonner": "2.0.6",
    "swr": "^2.3.0",
    "tailwindcss": "^4.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.10"
  },
