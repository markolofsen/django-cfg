{
  "name": "@djangocfg/og-image",
  "version": "1.2.0",
  "description": "Universal OG Image generation for Next.js apps",
  "author": {
    "name": "DjangoCFG",
    "url": "https://djangocfg.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./handler": {
      "types": "./dist/handler/index.d.ts",
      "import": "./dist/handler/index.js",
      "default": "./dist/handler/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "default": "./dist/components/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@vercel/og": "^0.8.5"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@djangocfg/typescript-config": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "^5.7.3"
  }
}
