{
  "name": "@djangocfg/api",
  "version": "1.2.0",
  "author": {
    "name": "DjangoCFG",
    "url": "https://djangocfg.com"
  },
  "license": "MIT",
  "description": "Auto-generated TypeScript API clients",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cfg/contexts": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cfg/generated": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cfg/generated/hooks": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cfg/generated/fetchers": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cfg/generated/schemas": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "check": "tsc --noEmit"
  },
  "peerDependencies": {
    "consola": "^3.4.2",
    "moment": "^2.30.1",
    "p-retry": "^7.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "swr": "^2.3.0",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@djangocfg/typescript-config": "workspace:*",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^8.5.0",
    "typescript": "5.8.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.11.1"
  }
}
