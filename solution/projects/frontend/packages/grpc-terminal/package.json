{
  "name": "@djangocfg/grpc-terminal",
  "version": "0.1.0",
  "description": "gRPC Terminal client with bidirectional streaming for Electron",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./grpc": {
      "import": "./dist/grpc/index.js",
      "types": "./dist/grpc/index.d.ts"
    },
    "./pty": {
      "import": "./dist/pty/index.js",
      "types": "./dist/pty/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check": "tsc --noEmit",
    "generate:proto": "../../apps/electron/node_modules/.bin/protoc --ts_out=./src/grpc/generated --ts_opt=generate_dependencies --ts_opt=long_type_string --proto_path=./src/grpc/proto ./src/grpc/proto/*.proto",
    "clean": "rm -rf dist",
    "postinstall": "electron-rebuild -f -w node-pty || true",
    "rebuild:pty": "electron-rebuild -f -w node-pty"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.2",
    "@protobuf-ts/runtime": "^2.11.1",
    "@protobuf-ts/runtime-rpc": "^2.11.1",
    "@protobuf-ts/grpc-transport": "^2.11.1",
    "node-pty": "^1.0.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.1",
    "@protobuf-ts/plugin": "^2.11.1",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "electron": ">=28.0.0"
  },
  "files": [
    "dist",
    "src/grpc/proto"
  ],
  "keywords": [
    "grpc",
    "terminal",
    "electron",
    "pty",
    "bidirectional-streaming"
  ],
  "license": "MIT"
}
