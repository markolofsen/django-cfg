  "version": "1.0.0",
  "private": true,
  "description": "DjangoCfg Desktop Application",
  "main": ".vite/build/main.js",
  "scripts": {
    "ai-docs": "djangocfg-docs",
    "postinstall": "mkdir -p node_modules && ln -sf ../../../node_modules/electron node_modules/electron 2>/dev/null || true && pnpm --filter @djangocfg/grpc-terminal rebuild:pty",
    "dev": "electron-forge start",
    "start": "electron-forge start",
    "build": "NODE_ENV=production electron-forge package",
    "build:dev": "electron-forge package",
    "package": "NODE_ENV=production electron-forge package",
    "package:dev": "electron-forge package",
    "make": "NODE_ENV=production electron-forge make",
    "publish": "NODE_ENV=production electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "check": "tsc --noEmit"
  },
  "keywords": [],
  "author": {
    "name": "Mark",
    "email": "markolofsen@gmail.com"
  },
  "license": "MIT",
